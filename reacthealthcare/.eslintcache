GridEmail\">\r\n                <Form.Label style={{color:\"white\"}}>Email</Form.Label>\r\n                <Form.Control\r\n                  type=\"email\"\r\n                  placeholder=\"Enter email\"\r\n                  name=\"email\"\r\n                  onChange={(e) => onChangeInput(e)}\r\n                 \r\n                />\r\n              </Form.Group>\r\n              </Col>\r\n              </Form.Row>\r\n\r\n              <Form.Row>\r\n              <Col md={12}>\r\n              <Form.Group as={Col} controlId=\"formGridPassword\">\r\n                <Form.Label style={{color:\"white\"}}>Password</Form.Label>\r\n                <Form.Control\r\n                  type=\"password\"\r\n                  placeholder=\"Password\"\r\n                  name=\"password\"\r\n                  onChange={(e) => onChangeInput(e)}\r\n                \r\n\r\n                />\r\n                \r\n              </Form.Group>\r\n              </Col>\r\n              </Form.Row>\r\n            <Button\r\n            className=\"ms-5 align-items-center\"\r\n              variant=\"primary\"\r\n              type=\"submit\"\r\n              value=\"button\"\r\n              onClick={(e) => onSubmit(e)}\r\n             \r\n            >\r\n             Submit\r\n            </Button>\r\n            \r\n          </Col>\r\n        </Row>\r\n       \r\n      </Form>\r\n      </>\r\n\r\n    )\r\n}\r\n","C:\\Users\\Taghreed Saeed\\Desktop\\React\\Front-End\\reacthealthcare\\src\\components\\LogIn.jsx",["51","52"],"C:\\Users\\Taghreed Saeed\\Desktop\\React\\Front-End\\reacthealthcare\\src\\components\\AuthRoute.jsx",[],{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":32,"column":24,"nodeType":"59","endLine":32,"endColumn":26,"suggestions":"60"},{"ruleId":"61","severity":1,"message":"62","line":2,"column":10,"nodeType":"63","messageId":"64","endLine":2,"endColumn":16},{"ruleId":"61","severity":1,"mes