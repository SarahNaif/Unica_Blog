osts/:id'>\n            <Show  setAuth = {setAuth}\n            user = {auth.currentUser}\n            selectArticle={selectArticle} \n             />\n          </Route>\n\n                    {/*---To the Edit user Profile---*/}\n         <Route path = \"/editprofile/:id\">\n           <EditProfile \n           auth={auth} />\n         </Route>\n\n\n                     {/*---To Create new post--*/}\n         <Route path = \"/new\">\n           <AddPost   auth={auth}  />\n         </Route>\n\n\n                      {/*---To show all posts---*/}\n         <Route exact path=\"/articles\">\n           <AllArticales />\n         </Route>\n\n    \n                       \n                       {/*---To login page --*/}\n        <Route path=\"/login\" >\n          <Login loginCallback={userLogin}/>\n        </Route>\n\n\n\n                       {/*---To signup page ---*/}\n        <Route path=\"/SignUp\" >\n          <SignUp loginCallback={userLogin} />\n        </Route>\n\n\n                       {/*---To The home page ---*/}\n        <Route exact path=\"/\">\n          <Home/>\n        </Route>\n      \n\n\n        <Footer/>\n\n    </Router>\n    }\n    </div>\n  );\n} \n\nexport default App;\n","C:\\Users\\Taghreed Saeed\\Desktop\\React\\Front-End\\reacthealthcare\\src\\components\\NavBar.jsx",["85","86"],"import React from 'react'\r\nimport { Navbar , Button} from 'react-bootstrap'\r\nimport { Link } from \"react-router-dom\";\r\nimport \"./Navbar.css\"\r\n\r\n\r\n  /* ===================\r\n      Navigation to pages \r\n     ===================*/\r\n\r\n     \r\nexport default function NavBar(props) {\r\n    return (\r\n        \r\n       \r\n      <nav class=\"navbar navbar-expand-lg navbar-light \">\r\n      <div class=\"container-fluid\">\r\n    <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarTogglerDemo03\" aria-controls=\"navbarTogglerDemo03\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n