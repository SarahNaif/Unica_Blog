                 <Form.Label style={{color:\"white\"}} >Type of the Article </Form.Label>\r\n                            <Form.Control as=\"select\" ustom onChange ={onChangeHandler}>\r\n                            {allSelect}\r\n                            </Form.Control>\r\n                        </Form.Group>\r\n                    </Form>\r\n                </Col>\r\n            </Row>\r\n            <Row className=\"justify-content-md-center\">\r\n                {allPosts.length !==0 ? allPosts : (<h1>Loading posts ... </h1>)}\r\n            </Row>\r\n        </Container>\r\n    )\r\n}\r\n","C:\\Users\\Taghreed Saeed\\Desktop\\React\\Front-End\\reacthealthcare\\src\\components\\OneCardArticle.jsx",["89","90"],"C:\\Users\\Taghreed Saeed\\Desktop\\React\\Front-End\\reacthealthcare\\src\\pages\\Home.jsx",["91"],"import React from 'react'\r\nimport Hero from \"../components/Hero\"\r\nimport Slider from \"../components/Slider\"\r\nexport default function Home() {\r\n    return (\r\n        <div>\r\n            <Hero/>\r\n            \r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\Taghreed Saeed\\Desktop\\React\\Front-End\\reacthealthcare\\src\\components\\LogIn.jsx",["92","93","94","95","96","97"],"import axios from \"axios\";\r\nimport React, { useState } from \"react\";\r\nimport { useHistory , Link} from \"react-router-dom\";\r\nimport { Form, Button, Col, Row , Alert } from \"react-bootstrap\";\r\nimport \"./Signup.css\"\r\nexport default function Login(props) {\r\n\r\n    /* =================\r\n     Hooks State  \r\n     ================= */\r\n  const history = useHistory();\r\n  const [credentials, setCredentials] = useState({ email: \"\", password: \"\" });\r\n\r\n    /* =================\r\n     Functions Handler  \r\n     ================= */\r\n  const onChangeInput = (event) => {\r\n    const { name, value } = event.target;\r\n    setCredentials({\r\n      ...credentials,\r\n      [name]: value,\r\n    });\r\n  };\r\n  const onSubmit = (event) => {\